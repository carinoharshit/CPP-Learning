// https://www.hackerearth.com/problem/algorithm/capitalbaazi/

#include<bits/stdc++.h>
using namespace std;

int main(){
    string s;
    getline(cin,s);
    
    for(int i=0;i<s.size();i++){
        if(islower(s[i])){
            cout<<char(s[i]-32);
        }
        if(s[i]==' ')
            cout<<endl;
    }
}


/* 

INPUT:
subscribe to the luv channel

OUTPUT: 
SUBSCRIBE
TO
THE
LUV
CHANNEL

*/
