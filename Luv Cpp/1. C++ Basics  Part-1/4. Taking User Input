#include<bits/stdc++.h> 
using namespace std;
int main()
{
    int a;
    double b;
    char c;

    cin >> a >> b >> c;
    cout << a << " " << b << " " << c << endl;

    cin >> c;
    cout << c << " " << (int)c;
}



/*
--------------------(INPUT)--------------------
22 9.9 d
9


--------------------(OUTPUT)--------------------
22 9.9 d
9 57
 
--------------------(THEORY)--------------------
- In line 12 we takes 9 integer value in char type c
- So it takes 57 acsii value of 9 and prints 57
*/ 
