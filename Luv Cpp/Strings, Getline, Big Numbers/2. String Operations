// ADD OPERATION

#include<bits/stdc++.h>
using namespace std;
int main(){
    string str1 = "Hello";
    string str2 = "World";
    string result = str1 +str2; // Add Operation
    cout << result;
}
// OUTPUT: HelloWorld
---------------------------------------

// Using == Relational Operator in String

#include<bits/stdc++.h>
using namespace std;
int main(){
    string str1 = "Hello";
    string str2 = "Hello";
    string result = str1 +str2; // Add Operation
    if(str1 == str2){
        cout << "Equal";
    }
    else{
        cout << "Not Equal";
    }
}
// OUTPUT: Equal
