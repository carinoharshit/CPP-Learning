// Pattern program using nested for loop...
#include<bits/stdc++.h>
using namespace std;

int main(){
    int n;
    cin >> n;
    for(int i=1; i<=n; ++i){
        for(int j = 1; j<=i; ++j){  // Nested loop
            cout << "*";
        }        
        cout << endl;
    }
}


/* 

INPUT:
5

OUTPUT: 
*
**
***
****
*****

*/
