//------------------------(BREAK)---------------------------
#include<bits/stdc++.h>
using namespace std;

int main(){
    for(int i=0; i<10; ++i){
        if(i == 5){
            break;   // breaks after condition true
        }
        cout << i << endl;
    }
}


/* 

INPUT:


OUTPUT: 
0
1
2
3
4

*/



//------------------------(CONTINUE)---------------------------
#include<bits/stdc++.h>
using namespace std;

int main(){
    for(int i=0; i<10; ++i){
        if(i == 5){
            continue;   // continue after condition true
        }
        cout << i << endl;
    }
}


/* 

INPUT:


OUTPUT: 
0
1
2
3
4
6
7
8
9

*/
