#include<bits/stdc++.h>
using namespace std;

int main(){
    int t; // testcase
    cin >> t;
    while(t--){
        int n; 
        cin >> n;
        if(n%2 == 0){
            cout << "YES" << endl;
        }
        else if(n%3 == 0){
            cout << "Divisible by 3"<< endl;
        }
        else{
            cout << "NO" << endl;
        }
    }
}


/* 

OUTPUT: 
3
1 2 3

INPUT:
NO
YES
Divisible by 3

*/
